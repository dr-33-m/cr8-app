# Frontend Environment Variables Configuration

# WebSocket Configuration
# URL for the WebSocket connection to the Cr8 Engine backend
VITE_WEBSOCKET_URL=

# API Configuration
# Base URL for the API endpoints
VITE_API_URL=

# Discord Integration (Optional)
# Discord webhook URL for notifications and feedback
VITE_DISCORD_WEBHOOK_URL=

# WebRTC Configuration
# WebRTC producer ID for Blender viewport streaming
VITE_WEBRTC_PRODUCER_ID=
# WebRTC signaling server URL for peer-to-peer connections
VITE_WEBRTC_SIGNALING_SERVER_URL=
